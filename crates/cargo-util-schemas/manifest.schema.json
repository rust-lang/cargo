    "TomlPlatform": {
      "description": "Corresponds to a `target` entry, but `TomlTarget` is already used.",
      "type": "object",
      "properties": {
        "dependencies": {
          "type": [
            "object",
            "null"
          ],
          "additionalProperties": {
            "$ref": "#/$defs/InheritableDependency"
          }

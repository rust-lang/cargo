{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "TomlManifest",
  "description": "This type is used to deserialize `Cargo.toml` files.",
  "type": "object",
  "properties": {
    "cargo-features": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "package": {
      "anyOf": [
        {
          "$ref": "#/$defs/TomlPackage"
        },
        {
          "type": "null"
        }
      ]
    },
    "project": {

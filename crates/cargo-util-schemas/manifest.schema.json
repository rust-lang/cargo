        "lib": {
          "description": "If set, the artifact should also be a dependency",
          "type": [
            "boolean",
            "null"
          ]
        },
        "target": {
          "description": "A platform name, like `x86_64-apple-darwin`",
          "type": [
            "string",
            "null"
          ]
        }
      }

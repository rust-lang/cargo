            {
              "type": "null"
            }
          ]
        },
        "repository": {
          "anyOf": [
            {
              "$ref": "#/$defs/InheritableField_for_string"
            },
            {
              "type": "null"
            }
          ]
        },
        "resolver": {
          "type": [
            "string",
            "null"
          ]
        },
